{
  "name": "next-web-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "NODE_OPTIONS='-r @newrelic/next' next dev",
    "dev:turbo": "NODE_OPTIONS='-r @newrelic/next' next dev --turbo",
    "start": "NODE_OPTIONS='-r @newrelic/next' next start",
    "lint": "next lint",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "build:analyze": "ANALYZE=true NEW_RELIC_ENABLED=false next build",
    "test": "TZ=Asia/Jakarta jest --watchAll",
    "test:coverage": "TZ=Asia/Jakarta jest --coverage",
    "test:dev": "TZ=Asia/Jakarta jest --watchAll --coverage --maxWorkers=20%",
    "test:clear": "jest --clearCache",
    "postinstall": "husky install",
    "generate:component": "node ./scripts/create-component",
    "generate:domain": "node ./scripts/create-domain",
    "generate:feature": "node ./scripts/create-feature",
    "generate:service": "node ./scripts/create-service",
    "generate:store": "node ./scripts/create-store",
    "generate:useCase": "node ./scripts/create-use-case"
  },
  "dependencies": {
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.14.18",
    "@mui/lab": "5.0.0-alpha.153",
    "@mui/material": "5.14.18",
    "@mui/system": "5.14.18",
    "@mui/x-date-pickers": "6.18.2",
    "@newrelic/next": "0.7.0",
    "@next/bundle-analyzer": "14.0.3",
    "@tanstack/react-query": "5.8.7",
    "@tanstack/react-query-devtools": "5.8.7",
    "axios": "1.6.2",
    "date-fns": "2.30.0",
    "firebase": "10.6.0",
    "material-ui-popup-state": "5.0.10",
    "newrelic": "11.5.0",
    "next": "14.0.3",
    "next-international": "1.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rey-mix": "git+ssh://git@github.com/REY-ID/rey-mix.git#v1.11.2",
    "sharp": "0.32.6",
    "zod": "3.22.4",
    "zustand": "4.4.6"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@emotion/eslint-plugin": "11.11.0",
    "@svgr/webpack": "8.1.0",
    "@tanstack/eslint-plugin-query": "5.8.4",
    "@testing-library/dom": "9.3.3",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/jest": "29.5.10",
    "@types/new-relic-browser": "1.230.2",
    "@types/newrelic": "9.14.3",
    "@types/node": "20.9.0",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "colors": "1.4.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-testing-library": "6.2.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "ts-jest": "29.1.1",
    "typescript": "5.3.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --config ./.prettierrc --write"
    ],
    "**/*.{css,scss,md,html,json}": [
      "prettier --config ./.prettierrc --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
